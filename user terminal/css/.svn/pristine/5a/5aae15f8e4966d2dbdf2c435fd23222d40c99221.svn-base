/*base*/
button {
	border:none;
	padding:0;
}
.text-over {
	overflow: hidden;
	white-space: nowrap;
	text-overflow: ellipsis;
}
.flex-box {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
}
.flex1 {
	-webkit-box-flex: 1;
    -webkit-flex: 1;
    flex: 1;
}
/*base end*/
.head {
	width: 100%;
	height: 3.167rem;
	/*background-color: #f5f5f5;*/
	padding-top:1.458rem;
	position: relative;
	z-index: 9;
	display: -webkit-box;
	display: -webkit-flex;
    display: flex;
}
.back-btn-base {
	height: 1.75rem;
	background: url(../images/back-btn.png) no-repeat left center;
	background-size: auto 100%;
	padding-left: 1.5rem;
	color: #fff;
	font-size: 1.667rem;
	line-height: 1.667rem;
	margin:0.417rem 6.15% 0 3.8%;
}
.headlines-box {
	-webkit-box-flex: 1;
    -webkit-flex: 1;
	flex: 1;
	background: rgba(255,255,255,.15);
	border-radius: 0.32rem;
    box-sizing: border-box;
    overflow: hidden;
}
.headlines-box >div {
	display: -webkit-box;
	display: -webkit-flex;
    display: flex;
    padding:0.75rem 0.71rem;

}
.headlines-box>div  h5 {
	background: url(../images/toutiao.png) no-repeat left center;
	background-size: auto 100%;
	padding-left: 1.25rem;
	color: #fff;
	font-size: 1.25rem;
	font-family: "黑体";
    line-height: 1.75rem;
    padding-right: 0.7rem;
    border-right: 0.04rem solid #fff;
}
.headlines-box>div p {
    -webkit-box-flex: 1;
    -webkit-flex: 1;
    flex: 1;
    color: #fff;
    font-size: 1.25rem;
    padding: 0 0 0 0.35rem;
    height: 1.67rem;
    line-height: 1.67rem;
}
.more-btn-base {
	width: 5.66%;
	margin: 0 4.5% 0 5.5%;
	background: url(../images/more-manu.png) no-repeat center center;
	background-size: 100% auto;
}

.logo-link,
.hongbao-link {
	position: absolute;
	z-index: 99;
	top: 6.5rem;
	display: block;
}
.logo-link {
	left: 3.8%;
	/*width: 9.5%;*/
	width: 4.16rem;
}
.hongbao-link {
	right: 2.78%;
	/*width: 6.85%;*/
	width: 3.04rem;
}
.logo-link >img ,
.hongbao-link >img {
	display: block;
	width: 100%;
}

.shangquan-bottom {
	width: 100%;
	max-height: 20rem;
	background: rgba(255,255,255,.86);
	border-top-left-radius: 0.8rem;
	border-top-right-radius: 0.8rem;
	position: absolute;
	z-index: 9;
	bottom: 0;
	padding: 0 4.2%;
	box-sizing: border-box;
}
.swiper-btn {
	width: 8.9%;
	height: 0.5rem;
	border-radius: 0.2915rem;
	background: #c2c5be;
	display: block;
	margin:0.625rem auto 0;
}
.swiper-container {
	max-height: 9rem;
	margin-top: 1.167rem;
	transition: all .3s ease;
	border-bottom: 0.08rem solid #d4d3d1;
	 -webkit-transition: all .3s ease;
    -moz-transition: all .3s ease;
    transition: all .3s ease;
}
.swiper-container-hide {
	max-height: 0;
	margin-top: 0;
	border-bottom: none;
}
.swiper-container ul.swiper-wrapper {
	margin-bottom: 2.083rem;
}
.swiper-slide {
	border:0.24rem solid #a3a2a2;
  	box-sizing: border-box;		
}
.swiper-wrapper > li img {
	display: block;
	/*width: 95%;*/
	max-width: 8.25rem;
	max-height: 8.25rem;
	margin:0 auto;
}
.swiper-slide .text-over {
	width: 100%;
	position: absolute;
	left: 0;
	bottom: 0;
	box-sizing: border-box;
	padding: 0.2rem 0.4rem;
	background: rgba(0,0,0,.6);
	color: #fff;
	font-size: 0.417rem;
}
.business-circle-addr {
	padding-top: 0.833rem;
	padding-bottom: 0.833rem;
	margin-right: -4.2%;
}
.business-circle-addr >div {
	overflow: hidden;
	border-right: 0.04rem solid #ced2d3;
    padding-right: 0.4rem;
}
.business-circle-addr >div h5 {
	font-size: 1.75rem;
	font-weight: 700;
	color: #5c5c56;
	line-height: 1.5;
    margin-bottom: 0.6rem;
}
.business-circle-addr >div p {
	font-size: 1.5rem;
	height: 1.75rem;
	color: #858585;
	padding-left: 2.08rem;
	background: url(../images/addr-point.png) no-repeat left center / auto 100%;
	/*font-size: auto 100%;*/
	/*line-height: 1.5*/
	width: 100%;
    box-sizing: border-box;
}
.by-car-btn {
	width: 16.36%;
	display: block;
	background: url(../images/by-car.png) no-repeat center 0.5rem / 1.833rem auto;
	color: #858585;
	padding-top: 2.08rem;
	font-size: 1.417rem;
	color: #858585;
}

