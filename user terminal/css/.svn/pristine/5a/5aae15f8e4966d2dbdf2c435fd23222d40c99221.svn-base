/*-------right manu-*/
.component {
    position: absolute;
    right: 0;
}

.cn-wrapper,
.manu-list {
    overflow: hidden;
    position: absolute;
    z-index: 10;
    right: 0rem;
}
.manu-list {
	width: 0;
}
.cn-wrapper >ul,
.manu-list >ul {
	background: rgba(0,0,0,.4);
	border-radius: 15rem;
    -webkit-transform: scale(0.01);
    -ms-transform: scale(0.01);
    -moz-transform: scale(0.01);
    transform: scale(0.01);
    pointer-events: none;
    -webkit-transition: all .3s ease;
    -moz-transition: all .3s ease;
    transition: all .3s ease;
}
.opened-nav >ul {
  border-radius: 50%;
  overflow: hidden;
  pointer-events: auto;
  -webkit-transform: scale(1);
  -moz-transform: scale(1);
  -ms-transform: scale(1);
  transform: scale(1);
}


.cn-button {
    position: absolute;
    right: 0px;
    cursor: pointer;
    z-index: 11;
    background: #37a0eb url(../images/manu-icon.png) no-repeat 65% center / 37.8% auto;
    outline: none;
    border:none;
}
.component button.active {
	 background: #37a0eb url(../images/manu-close.png) no-repeat 65% center / 37.8% auto;
}

.manu-list >ul li {
	position: absolute;
}
.manu-list >ul li a {
	display: block;
}


