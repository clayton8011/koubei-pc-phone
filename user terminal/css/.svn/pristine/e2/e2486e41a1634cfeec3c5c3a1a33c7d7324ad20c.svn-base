.text-over {
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
}
.swiper-container{
	width: 100%;
	height: 29.17rem;
}
.det-data{
	width: 100%;
	/*height: 15.375rem;*/
}
.date{
	width: 94.6%;
	/*height: 100%;*/
	margin: 0 auto;
}
.date h1{
	width: 100%;
	padding-bottom: 2rem;
	/*height: 4rem;*/
	font-size: 1.58rem;
	font-weight: 900;
	line-height: 1.6;
}
.date p{
	font-size: 1.5rem;
	color: #999999;
	width: 100%;
	/*height: 1.5rem;*/
	overflow: hidden;
	white-space:nowrap;
	text-overflow: ellipsis;
}
.price{
	width: 100%;
	height: 4rem;
}
.price-left{
	width: 12.25rem;
	height: 100%;
	float: left;
	font-size: 1.67rem;
	color: #C41720;
	font-weight: 900;
	text-align: right;
	line-height: 4rem;
}
.price-right{
	width: 9.08rem;
	height: 100%;
	float: left;
	font-size: 1.25rem;
	color: #999999;
	font-weight: 900;
	text-align: right;
	line-height: 4rem;
	position: relative;
}
.heng{
	display: block;
	width: 5.7rem;
	border-bottom: 1px solid #999999;
	position: absolute;
	top: 2rem;
	right: 0;
}
.zhe{
	display: block;
	width: 2.8rem;
	height: 1.33rem;
	line-height: 1.33rem;
	background-color: #f13e3a;
	border-radius: 1.25rem;
	font-size: 1.17rem;
	color: white;
	text-align: center;
	position: absolute;
	top: 1rem;
	right: -3.5rem;
}
.det-discount{
	width: 100%;
	height: 2.5rem;
	display: -webkit-flex;
    /*display: flex;
    -webkit-justify-content: space-between;
    justify-content: space-between;*/
}
.det-discount li{
	float: left;
	height: 1.33rem;
	line-height: 1.33rem;
	font-size: 1.33rem;
	color: #999999;
	text-align: center;
	border-right: 0.08rem solid #999999;
	/*padding: 0 1rem;*/
	width: 4rem;
}
.det-discount li:first-child {
	width: 7rem;
}
.det-discount li:last-child{
	border-right: none;
	flex: 1;
}
.split{
	width: 100%;
	height: 2.5rem;
}
.detail{
	width: 100%;
	height: 2.9rem;
	display: -webkit-flex;
    display: flex;
    -webkit-justify-content: space-between;
    justify-content: space-between;
}
.detail li{
	width: 33%;
	line-height: 2.9rem;
	border-bottom: 0.04rem solid #e9e9e9;
	float: left;
	font-size: 1.25rem;
	color: #555555;
	background: url(../img/shu.png) no-repeat right;
	background-size: 2%  58%;
	text-align: center;
}
.detail li:last-child{
	background-image: none;
}
.detail .miao{
	color: #C41720;
	border-bottom: 0.08rem solid #C41720;
}
.chart{
	width: 100%;
	padding-top: 1rem;
}
.chart_one{
	/*width: 94.6%;
	margin: 0 auto;*/
}
.tuwen{
	width: 100%;
	height: auto;
	/*margin: 2.67rem 2rem 0rem;*/
}
.kou_top{
	display: block;
	width: 100%;
	height: 1.125rem;
	background: url(../img/kou_top.png) no-repeat;
	background-size: 100%;
}
.score{
	width: 100%;
	height: 5rem;
	border-bottom: 0.04rem solid #e9e9e9;
}
.score ul {
	width: 5.708rem;
	height: 1.58rem;
	margin: 1.75rem 0 0 1.125rem;
	float: left;
}
.score ul li{
	width: 1rem;
	height: 1rem;
	background: url(../img/pinfen.png) no-repeat;
	background-size: 100% 100%;
	float: left;
	margin: 0.35rem 0.04rem;
}
.level{
	float: left;
	margin-top: 1.75rem;
	margin-left: 0.83rem;
	font-size: 1.25rem;
	color: #333333;
}
.score_p{
	float: right;
	font-size: 1.167rem;
	color: #999999;
	margin: 1.75rem 1.25rem 0 0;
}
.comment{
	width: 100%;
}
.comment ul{
	width: 94.6%;
	margin: 0 auto;
}
.comment ul li{
	width: 100%;
	overflow: hidden;
}
.com-pic{
	display: block;
	width: 3.33rem;
	height: 3.33rem;
	border-radius: 0.2rem;
	overflow: hidden;
	margin: 1.375rem 0 0 0.33rem;
	float: left;
}
.com-right{
	float: right;
	width: 80%;
	border-bottom: 1px solid #e9e9e9;
}
.name{
	margin-top: 1.667rem;
	overflow: hidden;
}
.name span:first-child{
	font-size: 1.25rem;
    color: #808080;
    float: left;
}
.name span:last-child{
	font-size: 1rem;
    color: #999999;
    float: right;
}
.com-nr{
	font-size: 1.33rem;
	color: #333333;
	margin: 0.83rem 0 2rem 0;
}
.more{
	width: 97%;
	height: 4rem;
	float: right;
	border-top: 0.08rem solid #E9E9E9;
}
.more p{
	line-height: 4rem;
	font-size: 1.33rem;
	color: #333333;
	float: left;
}
.more a{
	display: block;
	width: 0.625rem;
	height: 1.04rem;
	background: url(../img/go.png) no-repeat;
	background-size: 100%;
	float: right;
	margin-top: 1.417rem;
	margin-right: 1.67rem;
}
.not_top{
	display: block;
	width: 100%;
	height: 1.125rem;
	background: url(../img/not_top.png) no-repeat;
	background-size: 100%;
}
.notice p{
	width: 93.7%;
	margin: 2.7rem auto;
	font-size: 1.25rem;
	color: #555555;
}
.notice img{
	display: block;
	width: 62.8%;
	margin: 0 auto;
	margin-bottom: 6.8rem;
}
footer{
	width: 100%;
	height: 4.708rem;
	border-top: 1px solid #D5D5D5;
	position: fixed;
	bottom: 0;
	left: 0;
	background-color: white;
}
footer span:first-child{
	display: block;
	width: 28.7%;
	height: 100%;
	background: #c41720;
	text-align: center;
	float: right;
	font-size: 1.5rem;
	line-height: 4.708rem;
	color: white;
	font-weight: 900;
}
footer span:nth-child(2){
	display: block;
	width: 29%;
	height: 100%;
	background: #ffb12e;
	text-align: center;
	float: right;
	font-size: 1.5rem;
	color: #333333;
	line-height: 4.708rem;
	font-weight: 900;
}
footer span:nth-child(3){
	display: block;
	width: 25%;
	height: 100%;
	padding-top: 0.75rem;
	box-sizing: border-box;
	text-align: center;
	line-height: 1.5rem;
	float: right;
	font-size: 0.917rem;
	color: #555555;
	font-weight: 900;
}
footer span:nth-child(3) a{
	display: block;
	width: 1.708rem;
	height: 1.7rem;
	background: url(../img/shop_cat.png) no-repeat;
	background-size: 100%;
	margin: 0 auto;
}
footer span:nth-child(4){
	display: block;
	width: 13%;
	height: 100%;
	padding-top: 0.83rem;
	box-sizing: border-box;
	text-align: center;
	line-height: 2rem;
	float: right;
	font-size: 0.917rem;
	color: #555555;
	font-weight: 900;
}
.xin{
	display: block;
	width: 1.5rem;
	height: 1.375rem;
	background: url(../img/cang.png) no-repeat;
	background-size: 100%;
	margin: 0 auto;
}
.hxin{
	display: block;
	width: 1.5rem;
	height: 1.375rem;
	background: url(../img/cang02.png) no-repeat;
	background-size: 100%;
	margin: 0 auto;
}
.layer{
	width: 100%;
	height: 100%;
	background-color: black;
	position: fixed;
	top: 0;
	left: 0;
	opacity: .8;
	display: none;
	z-index: 99;
}
.payment{
	width: 100%;
	height: 21.5rem;
	position: fixed;
	top: 30%;
	z-index: 999;
	display: none;
}
.close{
	display: block;
	width: 1.875rem;
	height: 1.875rem;
	background: url(../img/guan.png) no-repeat;
	background-size: 100%;
	float: right;
	margin-right: 1.125rem;
}
.pay_nr{
	width: 100%;
	/*height: 18.4rem;*/
	background-color: white;
	position: absolute;
	bottom: 0;
}
.pay-top{
	width: 100%;
	position: relative;
	border-bottom: 1px solid #d5d5d5;
	box-sizing: border-box;
	overflow: hidden;
}
.pay-no{
	display: block;
	width: 1.25rem;
	height: 1.25rem;
	background: url(../img/zhifu.png) no-repeat;
	background-size: 100%;
	position: absolute;
	top: 3.54rem;
	left: 1.2rem;
}
.pay-yes{
	display: block;
	width: 1.25rem;
	height: 1.25rem;
	background: url(../img/zhifu2.png) no-repeat;
	background-size: 100%;
	position: absolute;
	top: 3.54rem;
	left: 1.2rem;
}
.pay-img{
	display: block;
	width: 7.5rem;
	height: 7.5rem;
	float: left;
	padding-top: 0.417rem;
	margin-left: 3.08rem;
	margin-bottom: 1.54rem;
}
.pay-bt{
	width: 61%;
	font-size: 1.08rem;
	color: #333333;
	margin: 0.75rem 0.8rem;
    float: right;
}
.pay-rmb{
	height: 1.08rem;
	line-height: 1.08rem;
	color: #C41720;
	position: absolute;
	top: 6.7rem;
	left: 11.67rem;
}
.number{
	width: 6.875rem;
	height: 1.75rem;
	border: 0.04rem solid #d5d5d5;
	border-radius: 0.08rem;
	position: absolute;
	top: 5.958rem;
	right: 0.917rem;
}
.min{
	width: 27.8%;
	height: 100%;
	border: none;
	border-right: 0.08rem solid #d5d5d5;
	background-color: white;
	float: left;
	outline: none;
}
.quantity{
	width: 44%;
	height: 100%;
	border: none;
	text-align: center;
	outline: none;
}
.add{
	width: 27.8%;
	height: 100%;
	border: none;
	border-left: 0.08rem solid #d5d5d5;
	background-color: white;
	float: right;
	outline: none;
}
.pay-center{
	width: 100%;
	height: 4.17rem;
}
.pay-center span:first-child{
	display: block;
	width: 12.25rem;
	height: 100%;
	float: left;
	margin-left: 0.83rem;
	font-size: 1.08rem;
	color: #999999;
	line-height: 4.17rem;
	text-align: center;
	position: relative;
}
.pay-center span:first-child a{
	display: block;
	width: 1.67rem;
	height: 1.67rem;
	background: url(../img/baoyou.png) no-repeat;
	background-size: 100%;
	position: absolute;
	top: 1.125rem;
	left: 1.5rem;
}
.pay-center span:last-child{
	display: block;
	width: 9.08rem;
	height: 100%;
	float: left;
	font-size: 1.08rem;
	color: #999999;
	line-height: 4.17rem;
	text-indent: 2.75rem;
	position: relative;
}
.pay-center span:last-child a{
	display: block;
	width: 1.67rem;
	height: 1.67rem;
	background: url(../img/huodong.png) no-repeat;
	background-size: 100%;
	position: absolute;
	top: 1.125rem;
	left: 0.708rem;
}
.pay-btn{
	width: 100%;
	height: 4.708rem;
	line-height: 4.708rem;
	background-color: #C41720;
	color: white;
	text-align: center;
	font-size: 1.5rem;
	font-weight: 900;
}
@media screen and (device-width: 320px) and (device-height: 480px) and (-webkit-device-pixel-ratio: 2) {
	.pay-bt{
		width: 54%;
		font-size: 1.08rem;
		color: #333333;
		margin: 0.75rem 0.8rem;
	    float: right;
	}
}

@media screen and (device-width: 320px) and (device-height: 568px) and (-webkit-device-pixel-ratio: 2) {
	.pay-bt{
		width: 54%;
		font-size: 1.08rem;
		color: #333333;
		margin: 0.75rem 0.8rem;
	    float: right;
	}
}