/*base*/
html {
	height: 100%;
}
button {
	border:none;
	padding:0;
	background: none;
}
.text-over {
	overflow: hidden;
	white-space: nowrap;
	text-overflow: ellipsis;
}
.flex-box {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
}
.flex1 {
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	flex: 1;
}
/*base end*/
.head {
	width: 100%;
	height: 3.167rem;
	height: 5%;
	padding-top:1.71%;
	position: relative;
	z-index: 9;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
}
.back-btn-base {
	background: url(../images/back-btn.png) no-repeat left center;
	background-size: auto 1.5rem;
	padding-left: 1.5rem;
	color: #fff;
	font-size: 1.25rem;
	margin: 0 3% 0 2.8%;
}
.headlines-box {
	/*-webkit-box-flex: 1;
	-webkit-flex: 1;
	flex: 1;*/
	 background: rgba(255,255,255,.15);
    border-radius: 0.32rem;
    box-sizing: border-box;
    overflow: hidden;
    margin: 0 3.25%;
    width: 100%;
}
.headlines-box >div {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	padding: 2.5% 0.71rem;

}
.headlines-box>div  h5 {
	background: url(../images/toutiao.png) no-repeat left center;
	background-size: auto 1.5rem;
	padding-left: 1.25rem;
	color: #fff;
	font-size: 1.25rem;
	font-family: "黑体";
	line-height: 1.75rem;
	padding-right: 0.7rem;
	border-right: 0.04rem solid #fff;
}
.more-btn-base {
	width: 5.66%;
	margin: 0 4% 0 3%;
	background: url(../images/more-manu.png) no-repeat center center;
	background-size: 100% auto;
}

.btn-grp-l,
.btn-grp-r{
	position: absolute;
	z-index: 99;
	top: 8.16%;
	display: block;
}
.btn-grp-l{
	left: 3.29%;
	width: 9.07%;
}
.btn-grp-r {
	right: 2.13%;
	width: 9.72%;
}
.btn-grp-l a,
.btn-grp-r>a ,
.btn-grp-r>button{
	display: block;
	width: 100%;
	margin-bottom:1rem;
}
.btn-grp-l a >img ,
.hongbao-link >img {
	display: block;
	width: 100%;
}
.music-box {
	position: relative;
	margin-bottom: 0.5rem;
}
.music-list {
	position: absolute;
	left: 100%;
	top: 50%;
	z-index: 99;
	background: rgba(0,0,0,0.46);
	padding: 0.8rem 0;
	border-radius: 0.4rem;
	transition: all 1s ease;
	max-width: 700%;
}
.music-list li{
	color: #fff;
	max-width: 100%;
	font-size: 1.25rem;
	padding-left: 1rem;
	padding-right: 0.75rem;
	margin-bottom: 1rem;
	text-align: left;
	background: url(../images/music-li.png) no-repeat left center /auto 1.5rem;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}
.music-list li:last-child {
	margin-bottom: 0;
}
.music-list li.active{
	background: url(../images/music-ac.png) no-repeat left center /auto 1rem;
}
.music-anim {
	transform: translateX(-500px);
}
.base-bottom {
	width: 100%;
	/*max-height: 20rem;*/
	background: rgba(255,255,255,.86);
	border-top-left-radius: 0.8rem;
	border-top-right-radius: 0.8rem;
	position: absolute;
	z-index: 9;
	bottom: 0;
	/*padding: 0 4.2%;*/
	box-sizing: border-box;
}
.scene-bottom {
	/*height: 25.84%;*/
	overflow: hidden;
}
.swiper-down-btn {
	width: 8.7%;
	height: 0.8rem;
	background: url(../images/swiper-btn.png) no-repeat center /100% auto;
	display: block;
	margin:0 auto 0;
}
.swiper-down-btn>img {
	display: block;
	width: 100%;
	height: auto;
}
.swiper-up-btn {
	width: 8%;
	height: 0.8rem;
	/*background: url(../images/swiper-up.png) no-repeat center /100% 100%;*/
	display: block;
	margin:0 auto 0;
}
.swiper-container {
	max-height: 9rem;
	margin-top: 1.167rem;
	transition: all .3s ease;
	/*border-bottom: 0.08rem solid #d4d3d1;*/
	-webkit-transition: all .3s ease;
	-moz-transition: all .3s ease;
	transition: all .3s ease;
}
#toutiao-sw{
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	flex: 1;
	color: #fff;
	font-size: 1.25rem;
	padding: 0 0 0 0.35rem;
	height: 1.67rem;
	line-height: 1.67rem;
	margin-top: 0;
	border: none;
}
#toutiao-sw .swiper-slide {
	border: none;
}
#toutiao-sw .swiper-slide a{
	color: #FFFFFF;
	display: block;
	width: 100%;
	height: 100%;
}
.swiper-container-hide {
	max-height: 0;
	margin-top: 0;
	border-bottom: none;
}
.swiper-container ul.swiper-wrapper {
	margin-bottom: 2.083rem;
}
.swiper-slide {
	border:0.16rem solid #a3a2a2;
	box-sizing: border-box;
}
.swiper-wrapper > li img {
	display: block;
	/*width: 95%;*/
	max-width: 8.25rem;
	max-height: 8.25rem;
	margin:0 auto;
}
.swiper-slide .text-over {
	width: 100%;
	position: absolute;
	left: 0;
	bottom: 0;
	box-sizing: border-box;
	padding: 0.2rem 0.4rem;
	background: rgba(0,0,0,.6);
	color: #fff;
	font-size: 1rem;
	text-align: center;
}
.base-addr {
	margin-right: -4.2%;
	height: 9.6%;
	overflow: hidden;
	box-sizing: border-box;
}
.scene-addr {
	height: 37.19%;
	height: 5.5rem;
	/*padding: 0.4rem 0;*/
	padding: 1.5% 0 1.5% 4.2%;
	box-sizing: border-box;
	width: 100%;
}

.base-addr >div {
	overflow: hidden;
	border-right: 0.04rem solid #ced2d3;
	padding-right: 0.4rem;
	position: relative;
}
.scene-addr >div{
	height: 100%;
	/*margin-top: 10%;*/
}
.base-addr >div h5 {
    font-size: 1.5rem;
    font-weight: bold;
    color: #333333;
    line-height: 1.5;
    /* margin-bottom: 0.5rem; */
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}
.base-addr >div p {
	font-size: 1.25rem;
    /* height: 1.3rem; */
    color: #858585;
    padding: 0.2rem 1rem 0.2rem 1.5rem;
    background: url(../images/addr-point.png) no-repeat left center;
    background-size: auto 1.5rem;
    line-height: 1;
    width: 100%;
    box-sizing: border-box;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    position: absolute;
    bottom: 0.1rem;
}
.scene-addr >button {
	height: 0%;
}
.by-car-btn {
	width: 19.76%;
    display: block;
    background: url(../images/by-car.png) no-repeat center 0.5rem / 1.5rem auto;
    padding-top: 2.08rem;
    font-size: 1.25rem;
    color: #858585;
}
.cn-button {
	position: absolute;
	width: 12%;
	height: 9%;
	top: 61.2%;
	border-top-left-radius: 2.4rem;
	border-bottom-left-radius: 2.4rem;
	right: 0px;
	cursor: pointer;
	z-index: 11;
	background: #37a0eb url(../images/manu-icon.png) no-repeat 65% center / 37.8% auto;
	outline: none;
	border:none;
}
.bus-cir-img {
    display: block;
    width: auto;
    height: 100%;
    margin-right: 3.34%;
	/*margin-top: 10%;*/
}
#shop-list {
	margin-top: 0;
}
.swiper-container ul.swiper-wrapper {
	margin-bottom: 0.9rem;
	margin-top: 0.4rem;
	transition: all 0.5s ease;
}
#scenes-list {
	margin-top: 0;
	height: 44.83%;
	overflow: hidden;
	transition: height 0.5s ease;
	overflow: hidden;
}
#scenes-list ul.swiper-wrapper {
	margin-bottom: 0.2rem;
}
.line {
	margin: 0 -4.3%;
	height: 1px;
	border-bottom: 0.04rem solid #d4d3d1;
}
.bottom-top{
	padding: 2% 4.2% 0;
	/*height: 62.4%;*/
	box-sizing: border-box;
	position: relative;
}
.pano-bg-box {
	position: absolute;
	top: 0;
	width: 100%;
	height: 8.78%;
	background: url(../images/pano-top-bg.png) repeat-x;
	background-size: auto 100%;
}


/*-----景点--*/
.spot-line {
	width: 100%;
	margin: 0;
}
.scenic-spot-bottom {
	height: 51.5%;
	z-index: 99;
	transition: all 0.8s ease;
}
.scenic-show-all {
	height: 70%;
}
.scenic-half-height {
	height: 51.5%;
}
.scenic-short-height {
	height: 23.5%;
}
.scenic-spot-addr>h5 {
	font-size: 1.75rem;
	font-weight: 700;
	color: #333333;
	line-height: 1.5;
	margin-bottom: 0.5rem;
}
#scenic-spot-list {
	padding: 0.3rem 0;
	margin-top: 0;
}
.swiper-container  ul.scenic-spot-sw-list {
	margin-bottom: 0.4rem;
}
.scenic-spot-detial {
	height: 54%;
	border-bottom: 0.75rem solid #ededec;
	background: rgba(255,255,255,.96);
	padding: 1.5rem 4.2%;
	font-size: 1.25rem;
	box-sizing: border-box;
	transition: all 0.8s  ease;
}
.scenic-show-all .scenic-spot-detial {
	height: 66%;
}
.hide-detial {
	height: 0;
	border: none;
	padding: 0;
	overflow: hidden;
}
.scenic-spot-detial b{
	padding-bottom: 0.5rem;
	display: block;
}
.scenic-spot-detial p{
	overflow : hidden;
	text-overflow: ellipsis;
	display: -webkit-box;
	-webkit-line-clamp:3;
	-webkit-box-orient: vertical;
	line-height: 1.5;
}
.scenic-show-all .scenic-spot-detial p {
	-webkit-line-clamp: inherit;
	overflow: auto;
	/*height: 15rem;*/
	height: 70%;
	transition: all 0.8s  ease;
}
.show-all-btn,
.show-all-inf-btn{
	display: block;
	width: 100%;
	font-size: 1.25rem;
	color: #8a8a8a;
	text-align: center;
	margin-top: 2rem;
}
.show-all-btn {
	margin-top: 1.5rem;
}
.show-all-btn img ,
.show-all-inf-btn img{
	height: 0.6rem;
	width: auto;
	margin-top: 0.55rem;
	margin-left: 0.5rem;
	transition: all 1s ease;
}
.showed img{
	transform: rotate(180deg);
}

.swiper-box {
	padding: 0 4.2%;
}
.scenic-area-list .active ,
.scenic-spot-sw-list .active {
	border: 0.16rem solid #37a0eb;
}
.spot-back-btn {
    width: 2.45rem;
    height: 2.25rem;
    background: url(../images/sport-back.png) no-repeat center /auto 100%;
    float: right;
    position: absolute;
    right: 4%;
    /* margin-top: -0.2rem; */
    top: 1.3rem;
}

/*-----------toutiao----------*/
.base-high-bottom {
	height: 77.15%;
	overflow: hidden;
	z-index: 999;
	transition: all 1s ease;
}
.base-high-bottom-box {
	/*border-top: 1px solid #cccbcb;*/
	background: #fff;
	height: 84.8%;
	overflow: auto;
	box-sizing: border-box;
	border-radius: 0;
	transition: all 0.6s ease;
}
.base-high-bottom h2 {
	padding-top: 2%;
	font-size: 1.5rem;
	font-weight: bold;
	text-align: center;
	color: #303131;
	padding-bottom: 3%;
	border-bottom: 0.04rem solid #e8e9e9;
}
.base-high-addr {
	/*height: 12.6%;*/
	height: 5.5rem;
	/*padding: 0.5% 0;*/
	padding: 1.5% 0 1.5% 4.2%;
	width: 100%;
}

.headlines-list li{
	padding-top: 1rem;
	padding-left: 2.53%;
	box-sizing: border-box;
	width: 100%;
}
.headlines-list li a{
	/*display: block;*/
	width: 100%;
	height: 100%;
}
.headlines-list li a>img {
    width: 7.5rem;
    height: 7.5rem;
    display: block;
    max-height: 11.5rem;
    margin-right: 2.53%;
    padding-bottom: 1rem;
}
.headlines-list li a>div {
	border-bottom: 0.04rem solid #eaeef5;
	/*padding-bottom: 1.25rem;*/
}
.headlines-detail {
    width: 85.3%;
    position: relative;
    overflow: hidden;
    box-sizing: border-box;
}
.headlines-detail>h5 {
	font-size: 1.4rem;
	color: #454545;
	overflow : hidden;
	text-overflow: ellipsis;
	display: -webkit-box;
	-webkit-line-clamp: 1;
	-webkit-box-orient: vertical;
	margin-top: -0.1rem;
	margin-right: 0.1rem;
}
.activity-detail>h5 {
	margin-bottom: 0.5rem;
}
.headlines-link {
	display: block;
    font-size: 1rem;
    color: #93a0af;
    background: url(../images/link-img.png) no-repeat left center/auto 1rem;
    padding-left: 2rem;
    margin: 1.3rem 0 1rem;
    line-height: 1;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    width: 100%;
    box-sizing: border-box;
}

.headlines-link span {
	padding-left: 20%;
}
.headlines-time,
.activity-time {
    font-size: 1.125rem;
    color: #73777b;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    position: absolute;
    bottom: 0.9rem;
    margin-right: 0.1rem;
}
.activity-time {
	 -webkit-line-clamp: 3;
	 position: relative;
	 bottom: auto;
}
.headlines-more {
	width:12.6%;
	display: block;
	height: 7.5rem;
	background: url(../images/tourtiao-more.png) no-repeat center/25% auto;
}

/*scenic-inf---*/
.scenic-inf-box >div {
	padding: 2rem 4.2% 1.5rem;
	border-bottom: 0.75rem solid #ededed;
}
.inf-base-tip {
	font-size: 1.25rem;
	margin-bottom: 1rem;
}
.inf-base-tip-last {
	margin-bottom: 0;
}
.inf-base-tip>img{
	width: 1.5rem;
	height: 1.5rem;
	padding-right: 1rem;
	vertical-align: text-bottom;
}
.inf-base-tip b{
	padding-right: 0.4rem;
}
.inf-title ,
.activity-section >h5 {
	font-size: 1.25rem;
	font-weight: bold;
	margin-bottom: 0.5rem;
}
.inf-detial {
	font-size: 1.25rem;
	line-height: 1.5;
}
.scenic-introduction {
	overflow : hidden;
	text-overflow: ellipsis;
	display: -webkit-box;
	-webkit-line-clamp: 3;
	-webkit-box-orient: vertical;
	transition: all 0.6ss ease;
	/*height: 5.5rem;*/
}
.inf-showed {
	height: auto;
	overflow: initial;
	-webkit-line-clamp: auto;
	display: block;
}

/*--activity-content-----------*/
.activity-head {
	position: absolute;
	width: 100%;
	top: 0;
}
.activity-inf-box .activity-section {
	padding: 2rem 4.2% 1.5rem;
}
.activity-gr-line {
	background: #f7f9fb;
	height: 0.75rem;
	width: 100%;
	border-bottom:0.04rem solid  #e9e9e9;
}
.activity-section>p{
	-webkit-line-clamp: auto;
	font-size: 1.25rem;
	line-height: 1.5;
}
#other-activity-list .swiper-slide {
	border: none;
}
#other-activity-list p{
	text-align: center;
	font-size: 1rem;
	color: #333333;
	overflow: hidden;
	white-space: nowrap;
	text-overflow: ellipsis;
}
#other-activity-list img {
	border-radius: 4px;
}
.inf-base-tip .line-more {
	width: auto;
	float: right;
	height: 1.25rem;
	margin-top: 0.4rem;
}
/*modal*/
.activity-modal {
	background: rgba(0,0,0,.6);
	position: fixed;
	top: 0;
	right: 0;
	bottom: 0;
	left: 0;
	z-index: 999;
	display: none;
}
.ac-modal-conetent{
	width: 92%;
	margin: 20% auto 0;
}
.ac-modal-box {
	background: #fff;
	border-radius: 0.4rem;
}
.ac-modal-head {
	padding: 1.5rem 3% ;
	border-bottom: 0.04rem solid #e7e7e7;
	font-size: 1.5rem;
	color: #333333;
}
.ac-moal-detial {
	padding: 0.75rem 3%;
}
.ac-moal-detial>h5 {
	font-weight: bold;
	font-size: 1.25rem;
}
.ac-moal-detial>p{
	font-size: 1rem;
	color: #666666;
}
.ac-moal-detial .last-ac-de-row {
	margin-bottom: 2.25rem;
}
.ac-close-btn {
	display: block;
	width: 10%;
	height: 5rem;
	background: url(../images/modal-ac-close.png) no-repeat center;
	background-size: 100% auto;
	margin: 1rem auto 0;
}

/*共用所有模块*/
.base-high-bottom,
.businessmen-box,
.activity-box,
.headlines-show-box,
.public-box,
.theme-box {
	display: none;
}
/*主题推荐*/
.show-part {
	display: block;
}


/*headlines-----*/
.headlines-page-body {
	background: #f6f8fa;
}
.headlines-head {
	position: relative;
	z-index: 9;
	height: 5.5rem;
	padding: 2rem 3% 1rem;
	box-sizing: border-box;
	box-shadow: 0 2px 3px rgba(0,0,0,.2);
	background: #fff;
}
.headlines-hd-back,
.public-head>button  {
	display: block;
	float:left;
	width: 1.25rem;
	height: 2.33rem;
	background: url(../images/hd-back.png) no-repeat left center/100% auto;
}
.headlines-hd-more {
	display: block;
	width: 1.25rem;
	height: 2.33rem;
	background: url(../images/hd-more.png) no-repeat left center/100% auto;
	float: right;
}
.headlines-body {
	padding: 2.125rem 3%;
	box-sizing: border-box;
}
.headlines-body >h1 {
	font-size: 2.08rem;
	color: #333333;
	text-align: center;
}
.headlines-body >h5 {
	font-size: 1.458rem;
	color: #999999;
	text-align: center;
	margin-top: 1rem;
}
.headlines-body>img {
	display: block;
	width: 100%;
	margin-top: 0.5rem;
	margin-bottom: 1.25rem;
}
.headlines-body>p {
	font-size:1.25rem ;
	color: #333333;
	text-indent: 2em;
	line-height: 1.5;
	margin-bottom: 0.5rem;
}
.headlines-body>p >img{
	display: block;
	width: 100%;
}
.headlines-body>p:last-of-type {
	margin-bottom: 2rem;
}
.turijian-read-box h5 {
	font-size: 1.5rem;
	color: #333333;
	border-left: 0.2rem solid #2f9eee;
	line-height: 1.1;
	padding-left: 0.4rem;
	margin-bottom: 1rem;
}
.turijian-read-list li{
	border-top: 0.04rem solid #dadada;
	padding: 0.4rem 0;
}
.turijian-read-list li a>img {
	display: block;
	width: 31.5%;
	max-width: 14rem;
	max-height: 7rem;
	margin-right: 3%;
}
.read-right-box {
	position: relative;
}
.read-right-box >p {
	font-size: 1.25rem;
	color:#333333 ;
	overflow : hidden;
	text-overflow: ellipsis;
	display: -webkit-box;
	-webkit-line-clamp:2;
	-webkit-box-orient: vertical;
}
.read-right-box >span {
	position: absolute;
	width: 100%;
	bottom: 0;
	left: 0;
	font-size:1rem ;
	color: #999999;
	text-align: right;
}


/*public-----------*/
.public-head {
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	height: 5.5rem;
	z-index: 9;
	padding: 2rem 3% 1rem;
	box-sizing: border-box;
	/*box-shadow: 0 2px 3px rgba(0,0,0,.2);*/
	background: rgba(255,255,255,.9);
	border-bottom: 0.04rem solid #cacaca;
}
.public-head >p{
	text-align: center;
	font-size: 2rem;
	color: #333333  ;
}
.public-body {
	width: 100%;
	height: 100%;
	box-sizing: border-box;
}
.public-bottom  {
	position: absolute;
	left: 0;
	right: 0;
	bottom: 0;
	background: #fff;
	height: 4.25rem;
	border-top: 0.04rem solid #e1e1e1;
	z-index: 999;
	padding: 0.6rem 0;
	box-sizing: border-box;
}
.disability{
	background: url(../images/disability.png) no-repeat center/auto 100%;
}
.hospital{
	background: url(../images/hospital.png) no-repeat center/auto 100%;
}
.parking{
	background: url(../images/parking.png) no-repeat center/auto 100%;
}
.toilet{
	background: url(../images/toilet.png) no-repeat center/auto 100%;
}
.public-bottom >div.active:nth-child(1){
	background: url(../images/disability-ac.png) no-repeat center/auto 100%;
}
.public-bottom >div.active:nth-child(2){
