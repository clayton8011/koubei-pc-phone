}
#manu-list >li>div {
	background: url(../images/manu-line.png) no-repeat center 43px;
}
#manu-list >li>div >a {
	position: relative;
}
#manu-list >li>div >a:after {
	transition: all 0.4s ease;
	content: " ";
	position: absolute;
	right: 17px;
	top: 16px;
	display: block;
	width: 11px;
	height: 11px;
	background: url(../images/manu-more-down.png) no-repeat center center;
}
#manu-list >li>div>a.active:after {
	background: url(../images/manu-more-up-ac.png) no-repeat center center;
}
#manu-list >li>div>a.coll:after {
	background: url(../images/manu-more-up.png) no-repeat center center;
}
#manu-list >li:nth-child(1) >div >a {
	background: url(../images/shangquan.png) no-repeat 23px center;
	/*color: #fff;*/
}
#manu-list >li:nth-child(2) >div >a {
	background: url(../images/shop.png) no-repeat 23px center;
}
#manu-list >li:nth-child(3) >div >a {
