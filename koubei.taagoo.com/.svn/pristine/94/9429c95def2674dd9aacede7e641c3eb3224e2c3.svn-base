.circleProgress{
	width: 80px;
	height: 80px;
	border:10px solid transparent;
	border-radius: 50%;
	position: absolute;
	top:0;
}

.leftcircle{
	border-bottom:10px solid #fff;
	border-left:10px solid #fff;
	left:0;
	transform: rotate(225deg);
	transition: all 0.1s ease;
}
.rightcircle{
	border-top:10px solid #fff;
	border-right:10px solid #fff;
	right:0;
	transform: rotate(225deg);
	transition: all 0.4s ease 0.2s;
}

.leftcircle:after {
	content: " ";
	display: block;
	position: absolute;
	width: 10px;
	height: 10px;
	top: 51px;
    left: 49px;
	border-radius: 50%;
	background: #fff;
	/*visibility: hidden;*/
}

.rightcircle:after {
	content: " ";
	display: block;
	position: absolute;
	width: 10px;
	height: 10px;
	top: 0px;
    left: 1px;
	border-radius: 50%;
	background: #fff;
}
