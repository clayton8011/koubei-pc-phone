}

/*-----景点--*/
.scenic-spot-bottom {
	height: 48%;
	z-index: 99;
	transition: all 0.8s ease;
}
.scenic-show-all {
	height: 65%;
}
.scenic-spot-addr>h5 {
	font-size: 1.75rem;
    font-weight: 700;
    color: #333333;
    line-height: 1.5;
    margin-bottom: 0.5rem;
}
#scenic-spot-list {
	padding: 0.3rem 0;
	margin-top: 0;
}
.swiper-container  ul.scenic-spot-sw-list {
	margin-bottom: 0.4rem;
}
.scenic-spot-detial {
	height: 54%;
	border-bottom: 0.75rem solid #ededec;
	background: rgba(255,255,255,.96);
	padding: 1.5rem 4.2%;
	font-size: 1.25rem;
	box-sizing: border-box;
	transition: all 0.8s  ease;
}
.scenic-show-all .scenic-spot-detial {
	height: 66%;
}
.hide-detial {
	height: 0;
	border: none;
	padding: 0;
	overflow: hidden;
}
.scenic-spot-detial b{
	padding-bottom: 0.5rem;
	display: block;
}
.scenic-spot-detial p{
	overflow : hidden;
	text-overflow: ellipsis;
	display: -webkit-box;
	-webkit-line-clamp:3;
	-webkit-box-orient: vertical;
	line-height: 1.5;
}
.scenic-show-all .scenic-spot-detial p {
	-webkit-line-clamp: inherit;
	overflow: auto;
	height: 15rem;
	transition: all 0.8s 2s ease;
}
.show-all-btn,
.show-all-inf-btn{
	display: block;
	width: 100%;
	font-size: 1.25rem;
	color: #8a8a8a;
	text-align: center;
	margin-top: 2rem;
}
.show-all-btn img ,
.show-all-inf-btn img{
    height: 0.6rem;
    width: auto;
    margin-top: 0.55rem;
    margin-left: 0.5rem;
    transition: all 1s ease;
}
.showed img{
	transform: rotate(180deg);
}

.swiper-box {
	padding: 0 4.2%;
}
.scenic-area-list .active ,
.scenic-spot-sw-list .active {
	border: 0.16rem solid #37a0eb;
}
