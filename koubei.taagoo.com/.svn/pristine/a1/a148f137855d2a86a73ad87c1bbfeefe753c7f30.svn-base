/*base*/
button {
	border:none;
	padding:0;
	background: none;
}
.text-over {
	overflow: hidden;
	white-space: nowrap;
	text-overflow: ellipsis;
}
.flex-box {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
}
.flex1 {
	-webkit-box-flex: 1;
    -webkit-flex: 1;
    flex: 1;
}
/*base end*/
.head {
	width: 100%;
	height: 3.167rem;
	height: 5%;
	padding-top:1.71%;
	position: relative;
	z-index: 9;
	display: -webkit-box;
	display: -webkit-flex;
    display: flex;
}
.back-btn-base {
    background: url(../images/back-btn.png) no-repeat left center;
    background-size: auto 1.5rem;
    padding-left: 1.5rem;
    color: #fff;
    font-size: 1.25rem;
    margin: 0 3% 0 2.8%;
}
.headlines-box {
	-webkit-box-flex: 1;
    -webkit-flex: 1;
	flex: 1;
	background: rgba(255,255,255,.15);
	border-radius: 0.32rem;
