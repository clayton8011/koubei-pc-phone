    overflow: hidden;
}
.headlines-box >div {
	display: -webkit-box;
	display: -webkit-flex;
    display: flex;
    padding: 2.5% 0.71rem;

}
.headlines-box>div  h5 {
	background: url(../images/toutiao.png) no-repeat left center;
	background-size: auto 1.5rem;
	padding-left: 1.25rem;
	color: #fff;
	font-size: 1.25rem;
	font-family: "黑体";
    line-height: 1.75rem;
    padding-right: 0.7rem;
    border-right: 0.04rem solid #fff;
}
.more-btn-base {
	width: 5.66%;
	margin: 0 4% 0 3%;
	background: url(../images/more-manu.png) no-repeat center center;
	background-size: 100% auto;
}

.btn-grp-l,
.btn-grp-r{
	position: absolute;
	z-index: 99;
	top: 8.16%;
	display: block;
}
.btn-grp-l{
	left: 3.29%;
	width: 9.07%;
}
.btn-grp-r {
	right: 2.13%;
	width: 9.72%;
}
.btn-grp-l a,
.btn-grp-r>a ,
.btn-grp-r>button{
	display: block;
	width: 100%;
	margin-bottom:1rem;
}
.btn-grp-l a >img ,
.hongbao-link >img {
	display: block;
	width: 100%;
}
.music-box {
	position: relative;
	margin-bottom: 0.5rem;
}
.music-list {
	position: absolute;
	right: 50%;
	top: 110%;
	z-index: 99;
	background: #FFFFFF;
	transition: all 1s ease;
}
.music-anim {
	transform: scale3d(.1, .1, .1) translateY(-50px);
}
.base-bottom {
	width: 100%;
	/*max-height: 20rem;*/
	background: rgba(255,255,255,.86);
	border-top-left-radius: 0.8rem;
	border-top-right-radius: 0.8rem;
	position: absolute;
	z-index: 9;
	bottom: 0;
	/*padding: 0 4.2%;*/
	box-sizing: border-box;
}
.scene-bottom {
	/*height: 25.84%;*/
	overflow: hidden;
}
.swiper-down-btn {
	width: 8%;
	height: 0.5rem;
	background: url(../images/swiper-down.png) no-repeat center /100% 100%;
	display: block;
	margin:0 auto 0;
}
.swiper-up-btn {
	width: 8%;
	height: 0.5rem;
	background: url(../images/swiper-up.png) no-repeat center /100% 100%;
	display: block;
	margin:0 auto 0;
}
.swiper-container {
	max-height: 9rem;
	margin-top: 1.167rem;
	transition: all .3s ease;
	/*border-bottom: 0.08rem solid #d4d3d1;*/
	 -webkit-transition: all .3s ease;
    -moz-transition: all .3s ease;
    transition: all .3s ease;
}
#toutiao-sw{
    -webkit-box-flex: 1;
    -webkit-flex: 1;
    flex: 1;
    color: #fff;
    font-size: 1.25rem;
