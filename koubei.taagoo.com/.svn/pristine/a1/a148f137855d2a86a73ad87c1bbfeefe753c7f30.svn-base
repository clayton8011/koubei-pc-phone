	margin-bottom: 0.2rem;
}
.line {
	margin: 0 -4.3%;
	height: 1px;
	border-bottom: 0.04rem solid #d4d3d1;
}
.bottom-top{
	padding: 2% 4.2% 0;
	/*height: 62.4%;*/
